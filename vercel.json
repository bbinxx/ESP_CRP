{
    "rewrites": [
        {
            "source": "/set",
            "destination": "/api/set"
        },
        {
            "source": "/get",
            "destination": "/api/get"
        },
        {
            "source": "/log",
            "destination": "/api/log"
        },
        {
            "source": "/logs",
            "destination": "/api/logs"
        },
        {
            "source": "/(.*)",
            "destination": "/SERVER/index.html"
        }
    ]
}